<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

	<rule>
        <from>^/$</from>  
        <to>/article_home</to> 
    </rule>
    
    <rule>
        <from>^/home/([0-9]+)$</from>  
        <to>/article_home?index=$1</to> 
    </rule>

    <rule>
        <from>^/mm$</from>  
        <to>/article_mm</to> 
    </rule>
    
    <rule>
        <from>^/mm/$</from>  
        <to>/article_mm</to> 
    </rule>
    
    <rule>
        <from>^/mm/([0-9]+)$</from>  
        <to>/article_mm?index=$1</to> 
    </rule>
    
    <rule>
        <from>^/duanzi$</from>  
        <to>/article_duanzi</to> 
    </rule>
    
    <rule>
        <from>^/duanzi/$</from>  
        <to>/article_duanzi</to> 
    </rule>
    
    <rule>
        <from>^/duanzi/([0-9]+)$</from>  
        <to>/article_duanzi?index=$1</to> 
    </rule>
    
    <rule>
        <from>^/tag/([0-9]+).html$</from>  
        <to>/article_tag?id=$1</to> 
    </rule>
    
    <rule>
        <from>^/tag/([0-9]+)_([0-9]+).html$</from>  
        <to>/article_tag?id=$1&amp;index=$2</to> 
    </rule>
    
   <rule>
        <from>^/([0-9]+).html$</from>  
        <to>/article_show?id=$1</to> 
   </rule>
    
    
    <rule>
        <from>^/pre/([A-Za-z0-9]+)$</from>  
        <to>/article_preTuTiao?tid=$1</to> 
    </rule>

	<rule>
        <from>^/([A-Za-z0-9]+)$</from>  
        <to>/article_home</to> 
    </rule>
    
</urlrewrite>