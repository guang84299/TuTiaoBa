<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

	<rule>
        <from>^/$</from>  
        <to>/tutiao_home</to> 
    </rule>
    
    <rule>
        <from>^/([A-Za-z0-9]+)$</from>  
        <to>/tutiao_findTuTiao?tid=$1</to> 
    </rule>

</urlrewrite>