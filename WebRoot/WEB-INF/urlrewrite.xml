<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

	<rule>
        <from>^/$</from>  
        <to>/tutiao_home</to> 
    </rule>

    <rule>
        <from>^/mm$</from>  
        <to>/tutiao_mm</to> 
    </rule>
    
    <rule>
        <from>^/mm/([0-9]+)$</from>  
        <to>/tutiao_mm?id=$1</to> 
    </rule>
    
    <rule>
        <from>^/life$</from>  
        <to>/tutiao_life</to> 
    </rule>
    
    <rule>
        <from>^/life/$</from>  
        <to>/tutiao_life</to> 
    </rule>
    
    <rule>
        <from>^/life/([0-9]+)$</from>  
        <to>/tutiao_life?id=$1</to> 
    </rule>
    
    <rule>
        <from>^/mm/([0-9]+).html$</from>  
        <to>/tutiao_showmm?id=$1</to> 
    </rule>
    
    <rule>
        <from>^/mm/([0-9]+)_([0-9]+).html$</from>  
        <to>/tutiao_showmm?id=$1&amp;page=$2</to> 
    </rule>
    
    <rule>
        <from>^/life/([0-9]+).html$</from>  
        <to>/tutiao_showlife?id=$1</to> 
    </rule>
    
    <rule>
        <from>^/life/([0-9]+)_([0-9]+).html$</from>  
        <to>/tutiao_showlife?id=$1&amp;page=$2</to> 
    </rule>
    
    
    <rule>
        <from>^/pre/([A-Za-z0-9]+)$</from>  
        <to>/tutiao_preTuTiao?tid=$1</to> 
    </rule>

	<rule>
        <from>^/([0-9]+)$</from>  
        <to>/tutiao_home</to> 
    </rule>
    
</urlrewrite>